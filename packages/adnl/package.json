{
  "name": "@tonutils/adnl",
  "version": "0.0.0",
  "description": "An efficient and lightweight ADNL (Advanced Distributed Network Layer) over UDP client implementation for Node.JS, designed to facilitate seamless communication and interaction with the TON (The Open Network) ecosystem. This package enables developers to easily build and integrate TON-based applications, ensuring high performance, reliability, and scalability.",
  "keywords": [
    "ton",
    "adnl",
    "client",
    "nodejs",
    "the-open-network"
  ],
  "author": "thekiba <hr@thekiba.io>",
  "homepage": "",
  "license": "MIT",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest"
  },
  "dependencies": {
    "@types/node": "^18.15.13",
    "@tonutils/keys": "^0.0.0",
    "@tonutils/keyring": "^0.0.0",
    "@tonutils/tl": "^0.0.0",
    "@tonutils/config": "^0.0.0",
    "ton-crypto": "3.2.0",
    "ton-core": "0.49.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0"
  }
}
